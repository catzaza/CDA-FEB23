length_a = float(input("Enter the length of Rectangle A: "))
width_a = float(input("Enter the width of Rectangle A: "))
length_b = float(input("Enter the length of Rectangle B: "))
width_b = float(input("Enter the width of Rectangle B: "))

# Calculate the area of both rectangles
area_a = length_a * width_a
area_b = length_b * width_b

# Determine which is greater or if both are equal
if area_a == area_b:
    print("Both rectangles are equal")
elif area_a > area_b:
    print("Rectangle A is greater than Rectangle B")
elif area_a < area_b:
    print("Rectangle B is greater than Rectangle A")
